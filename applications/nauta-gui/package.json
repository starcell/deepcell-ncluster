{
  "name": "nauta-enterprise-gui",
  "version": "0.0.1",
  "scripts": {
    "start-dev": "concurrently --kill-others \"npm run server-dev\" \"npm run client-dev\"",
    "client-dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "server-dev": "NODE_ENV=dev nodemon ./api/server.js",
    "start-prod": "node ./api/server.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test-client": "npm run unit",
    "test-api": "nyc mocha ./api/test --recursive",
    "test": "npm run test-client && npm run test-api",
    "lint": "eslint --ext .js,.vue src test/unit",
    "build": "node build/build.js"
  },
